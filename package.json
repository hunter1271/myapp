{
  "name": "myapp",
  "version": "0.0.1",
  "author": "myappdev",
  "license": "UNLICENSED",
  "scripts": {
    "start": "yarn && webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "test": "yarn && yarn lint && yarn test:jest",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --ext .jsx --ext .js */**",
    "lint:css": "stylelint ./app/**/*.scss --syntax scss",
    "mock:prd": "json-server ./mocks/prd-db.js --watch --port 9000 --routes ./mocks/routes.json",
    "test:jest": "jest --config jest.config.json --no-cache",
    "build:inv": "webpack --config ./webpack/webpack.config.prod.js"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg|png)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest.transform.js"
    }
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-react-css-modules": "^3.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "classnames": "^2.2.5",
    "express": "^4.15.4",
    "history": "^4.6.3",
    "immutable": "^3.8.2",
    "invariant": "^2.2.2",
    "js-cookie": "^2.1.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "piping": "^1.0.0-rc.4",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "ramda": "^0.25.0",
    "react": "16.0.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-scrollbar": "^0.5.1",
    "react-text-truncate": "^0.12.0",
    "reactstrap": "^5.0.0-alpha.3",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-duck": "^1.0.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.4.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack-isomorphic-tools": "^3.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "0.28.7",
