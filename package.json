{
  "name": "cip-front",
  "version": "0.0.1",
  "author": "Innodata",
  "scripts": {
    "start": "webpack-dev-server",
    "start:mock": "concurrently --kill-others \"npm run mock:api\" \"npm run start\"",
    "lint": "eslint --max-warnings 0 --ext .jsx --ext .js app/**",
    "mock:api": "json-server ./mock/db.js --watch --port 9000 --routes ./mock/routes.json"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "antd": "^2.12.6",
    "babel-plugin-import": "^1.4.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "rc-queue-anim": "^1.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-jss": "^7.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scrollbar": "^0.5.1",
    "react-text-truncate": "^0.11.2",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "redux-duck": "^1.0.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.1.0",
    "file-loader": "1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "json-server": "^0.12.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "0.18.2",
    "url-loader": "0.5.8",
    "webpack": "3.5.1",
