{
  "presets": [
    ["es2015", { "modules": false }],
    "stage-2",
    "react",
    "react-boilerplate"
  ],
  "plugins": [
    "dynamic-import-webpack",
    "transform-export-extensions",
    ["import", { "libraryName": "antd", "style": "css" }]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
